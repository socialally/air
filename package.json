{
  "name": "air",
  "version": "0.3.0",
  "description": "Lightweight, modular DOM library",
  "author": "muji <noop@xpm.io>",
  "publishConfig": {
    "registry": "https://registry.socialally.co",
    "ca": null
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/socialally/air.git"
  },
  "dependencies": {
    "zephyr": "~1.2.7"
  },
  "devDependencies": {
    "browser-sync": "~2.8.2",
    "browserify": "~11.0.1",
    "chai": "~3.2.0",
    "del": "~1.2.0",
    "gulp": "~3.9.0",
    "gulp-eslint": "~1.0.0",
    "gulp-istanbul-report": "0.0.1",
    "gulp-mocha-phantomjs": "~0.8.1",
    "gulp-size": "~1.2.1",
    "gulp-sourcemaps": "~1.5.2",
    "gulp-uglify": "~1.2.0",
    "gulp-util": "~3.0.4",
    "istanbul": "~0.3.6",
    "merge": "~1.2.0",
    "mocha": "~2.2.5",
    "mocha-phantomjs-istanbul": "~0.0.2",
    "require-dir": "~0.3.0",
    "run-sequence": "~1.1.2",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0"
  },
  "scripts": {
    "docs": "npm run readme",
    "readme": "mdp --force -v",
    "lint": "gulp lint",
    "dist": "gulp dist",
    "clean": "gulp clean",
    "spec": "gulp spec",
    "instrument": "istanbul instrument --output instrument lib",
    "cover": "npm run instrument && gulp cover",
    "test": "gulp test"
  },
  "mdp": {
    "title": "Air",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/socialally/air",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "design.md",
          "usage.md",
          "install.md",
          "api.md",
          "plugins.md",
          "plugin.md",
          "compatibility.md",
          "developer.md",
          "roadmap.md",
          "license.md"
        ]
      }
    ]
  }
}
